CAAA,IAAA,CAAA,KAAA,EAAA,KAAA,CAAA,UAAA,CAAA,EAAA,CAAA,CAAA;AACA;AACA;AAlBA,GApBA;AAwCA,YAAA;AACA,IAAA,IAAA,EAAA,IADA;AAEA,IAAA,OAFA,mBAEA,EAFA,EAEA,IAFA,EAEA;AACA,UAAA,EAAA,CAAA,IAAA,IAAA,IAAA,CAAA,IAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,EAAA,CAAA,IAAA;AACA;AACA;AANA;AAxCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"my_foot\">\r\n    <mt-tabbar v-model=\"down\" fixed class=\"tabbar\">\r\n      <mt-tab-item id=\"index\">\r\n        首页\r\n        <img src=\"../../assets/img/index_yes.png\" slot=\"icon\" v-if=\"down=='index'\" >\r\n        <img src=\"../../assets/img/index_no.png\" slot=\"icon\" v-else >\r\n      </mt-tab-item>\r\n      <mt-tab-item id=\"car\">\r\n        买车\r\n        <img src=\"../../assets/img/car_yes.png\" slot=\"icon\" v-if=\"down=='car'\" >\r\n        <img src=\"../../assets/img/car_no.png\" slot=\"icon\" v-else >\r\n      </mt-tab-item>\r\n      <mt-tab-item id=\"money\">\r\n        卖车\r\n        <img src=\"../../assets/img/money_yes.png\" slot=\"icon\" v-if=\"down=='money'\" >\r\n        <img src=\"../../assets/img/money_no.png\" slot=\"icon\" v-else >\r\n      </mt-tab-item>\r\n      <mt-tab-item id=\"me\">\r\n        我的\r\n        <img src=\"../../assets/img/me_yes.png\" slot=\"icon\" v-if=\"down=='me'\" >\r\n        <img src=\"../../assets/img/me_no.png\" slot=\"icon\" v-else >\r\n      </mt-tab-item>\r\n    </mt-tabbar>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n  .my_foot {\r\n    color: #999;\r\n  }\r\n \r\n</style>\r\n\r\n<script>\r\nexport default {\r\n  data(){\r\n    return {\r\n      down:'index'\r\n    }\r\n  },\r\n  mounted(){\r\n   if(this.$route.path==\"/index\"){\r\n     this.down='index'\r\n   }  \r\n   if(this.$route.path=='/car'){\r\n     this.down='car'\r\n   }\r\n   if(this.$route.path=='/money'){\r\n     this.down='money'\r\n   }\r\n   if(this.$route.path=='/me'){\r\n     this.down='me'\r\n   }\r\n  },\r\n  watch:{\r\n    down(value){\r\n      // 跳转首页\r\n      if(value=='index'){\r\n        this.$router.push('/').catch(e=>{})\r\n      }\r\n      // 跳转买车页面\r\n      if(value=='car'){\r\n        this.$router.push('/car').catch(e=>{})\r\n      }\r\n      // 跳转卖车页面\r\n      if(value=='money'){\r\n        this.$router.push('/money').catch(e=>{})   \r\n      }\r\n      // 跳转我的页面\r\n      if(value=='me'){\r\n        this.$router.push('/me').catch(e=>{})\r\n      }\r\n    }\r\n  },\r\n  '$route':{\r\n    deep:true,\r\n    handler(to,from){\r\n      if(to.path != from.path){\r\n        this.$router.push(to.path)\r\n      }\r\n    }\r\n\r\n  }\r\n}\r\n</script>"],"sourceRoot":"src/components/MintUI/Home"}]}